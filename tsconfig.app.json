{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "types/env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/apis/**/*",
    "types/**/*.d.ts",
    "vite/**/*.ts",
    // 自动导包配置，导入vue和vue-router
    "./auto-imports.d.ts"
  ],
  // "lib": ["WebWorker", "DOM"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["element-plus/global"]
  }
}
